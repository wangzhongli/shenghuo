<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/line_guide_qinqing"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_bg"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/rl_set_family_num"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/common_title_bg_colors"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/sfnl_title"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@drawable/lin_common_bg_selector"
            android:gravity="center" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/top_back_btn1" />
        </LinearLayout>

        <Button
            android:id="@+id/btn_set_family_save"
            android:layout_width="80dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:background="@drawable/lin_common_bg_selector"
            android:onClick="btn_set_family_save"
            android:text="@string/_save"
            android:textSize="17sp"
            android:textColor="@color/text_white" />

        <TextView
            android:id="@+id/tv_set_family_num_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/btn_set_family_save"
            android:layout_toLeftOf="@+id/btn_set_family_save"
            android:layout_marginRight="15dp"
            android:gravity="center"
            android:text="@string/set_family_name"
            android:textColor="@color/text_white"
            android:textSize="18sp" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/set_family_num_bg_yellow"
                    android:text="@string/num_one" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view1"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >

                    <requestFocus />
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself_selector" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/set_family_num_bg_yellow"
                    android:text="@string/num_two" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/set_family_num_bg_yellow"
                    android:text="@string/num_three" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/num_four" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view4"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/num_five" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view5"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/num_six" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view6"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/num_seven" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view7"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/add_family_icon_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/num_eight" />

                <EditText android:textColor="@color/edittext_color"
                    android:id="@+id/num_view8"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_weight="1"
                    android:background="@drawable/common_edit_bg"
                    android:inputType="phone"
                    android:maxLength="21"
                    android:paddingLeft="5dp"
                    android:singleLine="true" >
                </EditText>

                <ImageView
                    android:id="@+id/iv_set_family_num_icon8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="15dp"
                    android:background="@drawable/btn_family_num_add_myself"
                    android:visibility="invisible" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:orientation="horizontal" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@drawable/notify_set_family" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="@string/notice_set_family_text" />
    </LinearLayout>

    <Button
        android:id="@+id/del_family_num"
        android:layout_width="195dp"
        android:layout_height="43dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/btn_red_bg_press_selector"
        android:onClick="delFamilyNum"
        android:text="@string/del_family_num"
        android:textColor="@android:color/white" />

</LinearLayout>