<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/common_title_bg_colors" >

        <LinearLayout
            android:id="@+id/line_google_all_obj_loc"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@drawable/lin_common_bg_selector"
            android:gravity="center" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/top_back_btn1" />
        </LinearLayout>

        <TextView
            android:id="@+id/gg_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/sild_all_location"
            android:gravity="center"
            android:textColor="@color/text_white"
            android:textSize="24sp" />

        <LinearLayout
            android:id="@+id/gatl_updata_all_tracker"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:background="@drawable/lin_common_bg_selector"
            android:gravity="center" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/lin_common_bg_selector"
                android:gravity="center"
                android:src="@drawable/all_obj_refresh_ic_default" />
        </LinearLayout>
    </RelativeLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >

        <FrameLayout
            android:id="@+id/gatl_MapRoot"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >
        </FrameLayout>

        <LinearLayout
            android:id="@+id/line_all_obj_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|bottom"
            android:layout_marginBottom="15dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/ib_map_fangda_all_obj_loc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/map_zoon_fangda_selector" />

            <ImageButton
                android:id="@+id/ib_map_suoxiao_all_obj_loc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/map_zoon_suoxiao_selector" />
        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="74dp"
    android:background="@color/common_title_bg_colors"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/gatl_icon"
            android:layout_width="74dp"
            android:layout_height="match_parent"
            android:background="@drawable/text_click"
            android:contentDescription="@string/image_content_description"
            android:src="@drawable/defualt_img" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="@dimen/loging_margin_default" >

            <TextView
                android:id="@+id/gatl_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="@string/last_address"
                android:textColor="@color/text_white"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/gatl_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textColor="@color/text_white"
                android:textSize="16sp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>