<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/common_title_bg_colors"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/history_line_location"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@drawable/lin_common_bg_selector"
            android:gravity="center" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/top_back_btn1" />
        </LinearLayout>

        <TextView
            android:id="@+id/gg_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="@string/sild_history_location"
            android:textColor="@color/text_white"
            android:textSize="24sp" />
        
        <TextView
            android:id="@+id/tv_clear_all"
            android:layout_width="50dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="@string/clear_all_history_location"
            android:textColor="#fff" />
        
    </RelativeLayout>

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >

        <FrameLayout
            android:id="@+id/ghl_MapRoot"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >
        </FrameLayout>

        <LinearLayout
            android:id="@+id/line_all_obj_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right|bottom"
            android:layout_marginBottom="60dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/ib_map_fangda_all_obj_loc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:background="@drawable/map_zoon_fangda_selector" />

            <ImageButton
                android:id="@+id/ib_map_suoxiao_all_obj_loc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/map_zoon_suoxiao_selector" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bhl_line_address"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="bottom"
            android:layout_marginBottom="1dp"
            android:alpha="8"
            android:background="#df13b5b1"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/ghl_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:textColor="@android:color/white"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/ghl_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="16sp" />
        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_gravity="bottom"
            android:background="@drawable/all_location_bottom_devider" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:background="@color/common_title_bg_colors"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/line_ghl_data_left"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/ghl_data_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/history_data_before_selector_ten"
                android:focusable="false"
                android:focusableInTouchMode="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/line_ghl_time_left"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/ghl_time_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/history_data_before_selector"
                android:focusable="false"
                android:focusableInTouchMode="false" />
        </LinearLayout>

        <TextView
            android:id="@+id/ghl_data"
            android:layout_width="74dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@android:color/white" />

        <LinearLayout
            android:id="@+id/line_ghl_time_right"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/ghl_time_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/history_data_after_selector"
                android:focusable="false"
                android:focusableInTouchMode="false" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/line_ghl_data_right"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center" >

            <ImageView
                android:id="@+id/ghl_data_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/history_data_after_selector_ten"
                android:focusable="false"
                android:focusableInTouchMode="false" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>